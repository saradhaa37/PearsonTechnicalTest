          <JSR223Assertion guiclass="TestBeanGUI" testclass="JSR223Assertion" testname="JSR223 Assertion" enabled="true">
            <stringProp name="cacheKey">true</stringProp>
            <stringProp name="filename"></stringProp>
            <stringProp name="parameters"></stringProp>
            <stringProp name="script">def text=prev.getResponseDataAsString()

def Emailmatch=text=~/emailUnavailable\&quot;:(\w+),/
if(Emailmatch.find())
{
def value=Emailmatch.group(1)
if(value==&quot;true&quot;)
{
	//log.error(&quot;NO match&quot;)
	log.info(&apos;------------------&apos;)
	AssertionResult.setFailure(true);
	log.info(&apos;email already exists log&apos;+text)
	AssertionResult.setFailureMessage(&quot;This email number already exists &quot;+vars.get(&apos;p_Email&apos;));
	//prev.setResponseMessage(&quot;NoMatchFound&quot;);
	prev.setStopThread(true);
}
}

def phoneNumberMatch=text=~/phoneUnavailable\&quot;:(\w+),/
if(phoneNumberMatch.find())
{
def value=phoneNumberMatch.group(1)
if(value==&quot;true&quot;)
{
	log.info(&apos;------------------&apos;)
	AssertionResult.setFailure(true);
	log.info(&apos;phone already exists log &apos;+text)
	AssertionResult.setFailureMessage(&quot;This phone number already exists +&quot;+vars.get(&apos;p_PhoneNumber&apos;));
}
}
</stringProp>
            <stringProp name="scriptLanguage">groovy</stringProp>
          </JSR223Assertion>
