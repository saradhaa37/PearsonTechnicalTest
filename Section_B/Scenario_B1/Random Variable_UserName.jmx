            <stringProp name="script">import java.lang.Math;

int min=4;
int max=7;
int range=max-min;
int nFirstNameLen=(int)(Math.random()*range)+min;
int nLastNameLen=(int)(Math.random()*range)+min;
String randomString=&quot;abcdefghijklmnopqrstuvwxyz&quot;;
StringBuilder sb_firstName=new StringBuilder(nFirstNameLen);
StringBuilder sb_lastName=new StringBuilder(nLastNameLen);

for(int i=0;i&lt;nFirstNameLen;i++)
{
int index=(int)(randomString.length()*Math.random());
sb_firstName.append(randomString.charAt(index));
}

for(int i=0;i&lt;nLastNameLen;i++)
{
int index=(int)(randomString.length()*Math.random());
sb_lastName.append(randomString.charAt(index));
}
vars.put(&quot;p_firstName&quot;,sb_firstName.toString());
vars.put(&quot;p_lastName&quot;,sb_lastName.toString());</stringProp>
